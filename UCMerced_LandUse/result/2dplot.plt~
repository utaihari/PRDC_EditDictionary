set key outside center top reverse horizontal
set size 0.7,0.7
set term postscript eps
set out '/dev/null'
plot  "$0" index 1 usi $$1+1:$$2+1 ti "build" with po pt 7 lc rgb "red" ps 0.7,"$0" index 2 usi $1:$2 ti "residense" with po pt 7 lc rgb "blue" ps 0.7,"$0" index 3 usi $1:$2 ti "forest" with po pt 7 lc rgb "green" ps 0.7,"$0" index 4 usi $1:$2 ti "intersection" with po pt 7 lc rgb "orange" ps 0.7,"$0" index 5 usi $1:$2 ti "river" with po pt 7 lc rgb "black" ps 0.7
set out "$3"
replot
